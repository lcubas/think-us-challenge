{
  "name": "think-us-challenge",
  "version": "1.0.0",
  "description": "Think Us Challenge Node.js/React",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --env-file=.env src/index.js",
    "migrate:up": "node --env-file=.env runMigrations.js up",
    "migrate:down": "node --env-file=.env runMigrations.js down",
    "lint": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "jest --runInBand --detectOpenHandles",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "api",
    "node",
    "express",
    "jwt"
  ],
  "author": "lcubas",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lcubas/think-us-challenge.git"
  },
  "bugs": {
    "url": "https://github.com/lcubas/think-us-challenge/issues"
  },
  "homepage": "https://github.com/lcubas/think-us-challenge#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
